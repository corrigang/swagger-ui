        {
            "path": "/sub/{sub-guid}/item/hphcodes",
            "operations": [
                {
                    "method": "GET",
                    "summary": "Get the full details of the of the item with the given HPH Code",
                    "nickname": "getItemDetails",
                    "parameters": [
                        {
                            "paramType": "path",
                            "name": "sub-guid",
                            "type": "string",
                            "required": true,
                            "description": "GUID of the subscription, from earlier subscription query"
                        },
                        {
                            "paramType": "query",
                            "name": "values",
                            "type": "string",
                            "required": false,
                            "description": "A comma-separated list of HPH Codes, such as 005VC9988,005VC9989,005VC9990"
                        },
                        {
                            "paramType": "body",
                            "name": "values",
