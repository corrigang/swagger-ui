            "operations": [
                {
                    "method": "GET",
                    "summary": "Get the associated URLs of the item, with the given HPH Code",
                    "nickname": "getItemUrls",
                    "parameters": [
                        {
                            "paramType": "path",
                            "name": "sub-guid",
                            "type": "string",
                            "required": true,
