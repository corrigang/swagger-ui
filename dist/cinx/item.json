{
    "basePath": "https://api.cinx.com",
    "swaggerVersion": "1.2",
    "apiVersion": "0.1",
    "resourcePath": "/item",
    "apis": [
        {
            "path": "/sub/{sub-guid}/item/hphcode/{hph-code}",
            "operations": [
                {
                    "method": "GET",
                    "summary": "Get the full details of the of the item with the given HPH Code",
                    "nickname": "getItemDetails",
                    "parameters": [
                        {
                            "paramType": "path",
                            "name": "sub-guid",
                            "type": "string",
                            "required": true,
                            "description": "GUID of the subscription, from earlier subscription query"
                        },
                        {
                            "paramType": "path",
                            "name": "hph-code",
                            "type": "string",
                            "required": true,
                            "description": "HPH Code, such as 005VC9988"
                        }
                    ],
                    "responseMessages": [
                        {
                            "code": 401,
                            "message": "Invalid user name/password"
                        }
                    ],
                    "notes": "Uses HTTP AUTH protocol."
                }
            ]
        },
        {
            "path": "/sub/{sub-guid}/item/hphcode/{hph-code}/price",
            "operations": [
                {
                    "method": "GET",
